{
  "hosting": {
    "public": "build",
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [{
          "key": "Cache-Control",
          "value": "no-cache"
        }]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=432000"
        }]
      }
    ]
  }
}
